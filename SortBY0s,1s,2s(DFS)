class Solution{
    public static void main(){
        int low= mid-1 ,mid= low+1,  high=n-1;

        while(mid<= high){
            if(arr[mid] == 0){
                swap(arr[mid], arr[low]);
                low++;

            }
            else if(arr[mid]== 1){
                swap(arr[mid] ,arr[low]);
                mid++;

            }
            else{
                swap(arr[mid] , arr[high]);
                high--;
            }
        }
    }
}